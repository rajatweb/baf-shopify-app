model ShopAsset {
  id              String    @id @default(cuid())
  shopifyFileId   String    @unique
  url             String    @unique
  type            AssetType
  mimeType        String?
  shop             String
  createdAt       DateTime  @default(now())
  updatedAt       DateTime  @updatedAt
  @@index([shop])
}

enum AssetType {
  IMAGE
  AUDIO
}
